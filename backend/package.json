{
  "name": "koe-forum-engine",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seeds/index.ts"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc -p . && tsc-alias",
    "dev": "tsx src/bin/www.ts",
    "watch:dev": "tsx watch src/bin/www.ts",
    "lint": "eslint ./src",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate deploy",
    "test:unit": "dotenv -e .env.test -- vitest --project unit",
    "test:integ": "dotenv -e .env.test -- vitest --project integration --no-file-parallelism"
  },
  "dependencies": {
    "@prisma/client": "5.19.0",
    "argon2": "0.31.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "16.3.1",
    "express": "4.16.1",
    "express-validator": "7.0.1",
    "jsonwebtoken": "9.0.1",
    "morgan": "1.9.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@testcontainers/postgresql": "10.12.0",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/morgan": "1.9.9",
    "@types/node": "20.11.5",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "7.4.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "prisma": "5.19.0",
    "sqlite3": "5.1.6",
    "supertest": "6.3.3",
    "testcontainers": "10.2.1",
    "tsc-alias": "1.8.8",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "typescript-eslint": "8.33.1",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.4.0"
  }
}
