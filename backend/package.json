{
  "name": "koe-forum-engine",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*.js": "./src/*.js"
  },
  "scripts": {
    "dev": "node src/bin/www.js",
    "watch:dev": "nodemon src/bin/www.js",
    "lint": "eslint ./src --ext .js",
    "test": "vitest",
    "migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:make --knexfile ./src/db/knexfile.js",
    "migrate:run": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./src/db/knexfile.js",
    "migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:down --knexfile ./src/db/knexfile.js",
    "seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:make --knexfile ./src/db/knexfile.js",
    "seed:run": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile ./src/db/knexfile.js"
  },
  "dependencies": {
    "argon2": "0.31.0",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "16.3.1",
    "express": "4.16.1",
    "jsonwebtoken": "9.0.1",
    "knex": "2.5.1",
    "morgan": "1.9.1",
    "pg": "8.11.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "eslint": "8.44.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "sqlite3": "5.1.6",
    "supertest": "6.3.3",
    "testcontainers": "10.2.1",
    "vitest": "0.33.0"
  }
}
